    "axios": "^1.6.5",
    "config": "^3.3.9",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-cron": "^3.0.3",
